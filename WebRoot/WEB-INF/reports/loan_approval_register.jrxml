	<queryString>
		<![CDATA[

SELECT r.LOAN,r.LOANDATE,r.LOANMONEY,r.LOANMONEYCAP,r.LOANREASON,r.USERNAME,r.ACCOUNT,r.BANK,r.LOANREMARK,
zjl.remark as zjl_remark,
zjl.remark2 as zjl_remark2,
zjl.operateDT as zjl_operateDT,
u1.name as zjl_name,
kj.remark as kj_remark,
kj.remark2 as kj_remark2,
kj.operateDT as kj_operateDT,
u2.name as kj_name,
cn.remark as cn_remark,
cn.remark2 as cn_remark2,
cn.operateDT as cn_operateDT,
u3.name as cn_name
from loan_approval_register r
left join process_history zjl on zjl.id = r.zjl_id
left join process_history kj on kj.id = r.kj_id
left join process_history cn on cn.id = r.cn_id
left join sys_user u1 on zjl.user_id = u1.id
left join sys_user u2 on kj.user_id = u2.id
left join sys_user u3 on cn.user_id = u3.id
where r.ID in ($P!{ids})]]>
	</queryString>
