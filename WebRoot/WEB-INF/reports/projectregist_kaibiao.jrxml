	<queryString>
		<![CDATA[SELECT
id AS `序号`,
(SELECT GROUP_CONCAT(s.text) FROM sys_dictionary s WHERE s.id in (company_id)) AS `公司名称`,
projectName AS `项目名称`,
(SELECT GROUP_CONCAT(s.text) FROM sys_dictionary s WHERE s.id in (province_id)) AS `省`,
(SELECT GROUP_CONCAT(s.text) FROM sys_dictionary s WHERE s.id in (city_id)) AS `市`,
(SELECT GROUP_CONCAT(s.text) FROM sys_dictionary s WHERE s.id in (county_id)) AS `县`,
(SELECT GROUP_CONCAT(s.text) FROM sys_dictionary s WHERE s.id in (qualifyRequirement)) AS `资质要求`,
(SELECT GROUP_CONCAT(s.text) FROM sys_dictionary s WHERE s.id in (bds)) AS `标段`,
registDT AS `开标时间`,
createDT AS `登记时间`,
bidder AS `项目经理`,
(SELECT GROUP_CONCAT(s.text) FROM sys_dictionary s WHERE s.id in (delegate_id)) AS `委托人`,
bsbz AS `标书编制`,
ysbz AS `预算编制`
FROM projectregist   $P!{whereSQL}]]>
	</queryString>
