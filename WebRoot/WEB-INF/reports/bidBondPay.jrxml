	<queryString>
		<![CDATA[select b.*,u1.name as applierName,u1.mobilePhone,u3.name as handlerName2,
u2.name as handlerName,bd.bidNames from bidbond as b
left join sys_user as u1 on b.applierId=u1.id
left join sys_user as u2 on b.handlerId=u2.id
left join sys_user as u3 on b.handlerId2=u3.id
left join
(
select b.id as uid,GROUP_CONCAT(d.text) as bidNames
from sys_dictionary d,bidbond b
WHERE find_in_set(d.id,(select b.bidSection from bidbond b WHERE b.id=$P{id}))
GROUP by b.id
) bd ON b.id = bd.uid
where b.id=$P{id}]]>
	</queryString>
