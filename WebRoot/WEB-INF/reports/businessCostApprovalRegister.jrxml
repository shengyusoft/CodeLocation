	</parameter>
	<queryString>
		<![CDATA[SELECT
	r.COSTNAME AS 费用名称,
	DATE_FORMAT(r.APPLYPAYDATE, '%Y-%c-%d') AS 申请支付时间,
	r.REASON AS 事由,
REPLACE(FORMAT(r.APPLYCOST,2),',','') AS 申请支付金额,
	r.APPLYCOST AS 申请支金额,
	r.APPLYCOSTCAP AS 大写,
	-- 收款人信息
	r.USERNAME AS 户名,
	r.BANK AS 开户行,
	r.ACCOUNT AS 账号,
	r.CONTACTPERSON AS 联系人,
	r.CONTACTWAY AS 联系方式,
	r.APPLICANT AS 申请人,
	DATE_FORMAT(r.APPLICANTDATE, '%Y-%c-%d') AS 申请时间,
	r.paymentRemark AS 备注,
(SELECT handler_name from v_process where process_id = r.processid and role_code='role_top_manger' limit 1) as 总经理,
(SELECT operateDT from v_process where process_id = r.processid and role_code='role_top_manger' limit 1) as 总经理操作时间,
(SELECT audit_option from v_process where process_id = r.processid and role_code='role_top_manger' limit 1) as 总经理意见,
(SELECT remark from v_process where process_id = r.processid and role_code='role_top_manger' limit 1) as 总经理备注,

(SELECT handler_name from v_process where process_id = r.processid and role_code='role_account' limit 1) as 会计,
(SELECT operateDT from v_process where process_id = r.processid and role_code='role_account' limit 1) as 会计操作时间,
(SELECT audit_option from v_process where process_id = r.processid and role_code='role_account' limit 1) as 会计意见,
(SELECT remark from v_process where process_id = r.processid and role_code='role_account' limit 1) as 会计备注,

(SELECT handler_name from v_process where process_id = r.processid and role_code='role_cashier' limit 1) as 出纳,
(SELECT operateDT from v_process where process_id = r.processid and role_code='role_cashier' limit 1) as 出纳操作时间,
(SELECT audit_option from v_process where process_id = r.processid and role_code='role_cashier' limit 1) as 出纳意见,
(SELECT remark from v_process where process_id = r.processid and role_code='role_cashier' limit 1) as 出纳备注
FROM business_cost_approval_register r where id in ($P!{ids})]]>
	</queryString>
